  "scripts": {
    "start": "craco start",
    "build": "craco build --verbose",
    "docs:build": "npx styleguidist build",
    "docs:start": "npx styleguidist server",
    "docs:start:prod": "npx styleguidist build & npx serve docs",
    "test": "craco test --updateSnapshot",
    "eject": "react-scripts eject",
    "lint-fix": "eslint '*/**/*.{ts,tsx}' --fix",
    "lint-check": "eslint '*/**/*.{ts,tsx}'",
    "coverage": "craco test --env=jsdom --watchAll  --collect-coverage --updateSnapshot",
    "deploy-local": "babel-node  ./deploy-dev.sh",
    "sitemap": "babel-node ./sitemap-generator.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --pattern '**/*.*(js|ts|tsx|less)' --staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "reportWebVitals.ts"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/types.ts",
      "!**/en.ts",
      "!**/ar.ts",
      "!**/API.ts",
      "!**/react-app-env.d.ts",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 10,
        "lines": 20,
        "statements": 20
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
